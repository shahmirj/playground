/**
 *  Exclusive-or gate: out = !(a == b).
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=n1);
    Or(a=a,b=b, out=o1);
    Nand(a=n1, b=o1, out=r);
    Not(in=r, out=out);

}
